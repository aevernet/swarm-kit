# APP ENVIRONMENT --------------------------------------------------
VERSION_CONTROL=simple
# PATHS ------------------------------------------------------------
SK_CFGDIR="$SK_OPT/cfg"
SK_INCDIR="$SK_OPT/inc"
SK_REGDIR="$SK_OPT/registry"
SK_DOCDIR="$SK_OPT/docs"
SK_SCRIPTS="$SK_OPT/scripts"
STACKS="$SK_SCRIPTS/stacks"
ELEM="$SK_SCRIPTS/elem"
PKGDIR="$SK_SCRIPTS/pkgs"
# FILES ------------------------------------------------------------
SK_DEF="$CFGDIR/.defaults"
SK_DIS="$CFGDIR/.defaults.dist"
SK_NET="$CFGDIR/.network"
SK_PASSWORD="/tmp/.pwd"

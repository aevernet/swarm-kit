# ------------------------------------------------------------------
# FUNCTIONS
# ------------------------------------------------------------------
#
# INSTALL FUNCTION
#
shellcheck-install()
{
    echoLog "spacer"

    aptInstall shellcheck

    echoLog "spacer"
}
#
# CONFIG FUNCTION
#
shellcheck-config()
{
    echoLog "spacer"

    echoLog "No Config Requirement"

    echoLog "spacer"
}
#
# REMOVE FUNCTION
#
shellcheck-remove()
{
    echoLog "spacer"

    aptRemove shellcheck

    echoLog "spacer"
}
